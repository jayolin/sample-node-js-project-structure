{
  "name": "bare-node-js-project-setup",
  "version": "1.0.0",
  "description": "Sample node js setup",
  "main": "src/bootstrap/start.js",
  "scripts": {
    "mongo-repset": "run-rs -v 4.0.3 --keep",
    "dev": "NODE_ENV=development nodemon -w src --exec \" babel-node src/bootstrap/start.js  \"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "prestart": "npm run -s build",
    "start:prod": "NODE_ENV=production pm2 reload api.config.js --env production",
    "start:dev": "NODE_ENV=development pm2 reload api.config.js --env development",
    "init:dev": "NODE_ENV=development babel-node src/scripts/init-platform.js",
    "init:prod": "NODE_ENV=production node dist/scripts/init-platform.js",
    "workers:dev": "NODE_ENV=development babel-node src/scripts/boot-workers.js && babel src -s -D -d dist --presets env,stage-0 && pm2 flush && pm2 reload workers.config.js --env development && pm2 logs --lines 200",
    "workers:prod": "NODE_ENV=production node dist/scripts/boot-workers.js && pm2 reload workers.config.js --env production",
    "compile-templates": "mjml src/templates/mjml/*.mjml -o src/templates/mjml-compiled && handlebars src/templates/mjml-compiled/ -f src/templates/compiled/templates.js -e html -m -c handlebars/runtime && echo \"Done compiling templates into 'templates/compiled/templates.js' \"",
    "lint": "eslint src",
    "unit-test": "NODE_ENV=test nodemon -w src/test --exec \"mocha --require babel-core/register \"src/test/unit/**/**/*.test.js\"\"",
    "integration-test": "NODE_ENV=test nodemon -w src/test --exec \"mocha --require babel-core/register \"src/test/integration/**/**/*.test.js\"\"",
    "test-coverage-unit": "NODE_ENV=test istanbul cover _mocha -- --require babel-core/register \"src/test/unit/**/**/*.test.js\"",
    "test": "NODE_ENV=test nyc  mocha \"src/test/unit/**/**/*.test.js\"",
    "coverage-report": "nyc report --reporter=text-summary",
    "mjml": "mjml src/templates/mjml/*.mjml -o src/templates/mjml-compiled"
  },
  "dependencies": {
    "amqplib": "^0.5.3",
    "auto-bind-inheritance": "^1.0.6",
    "awilix": "^3.0.6",
    "awilix-express": "^1.0.0",
    "aws-sdk": "^2.389.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "dotenv-extended": "^2.0.1",
    "express": "^4.16.2",
    "express-joi-validation": "^0.2.1",
    "fast-csv": "^2.4.1",
    "flat": "^4.1.0",
    "form-data": "^3.0.0",
    "handlebars": "^4.1.0",
    "helmet": "^3.12.0",
    "http-status-codes": "^1.3.0",
    "javascript-time-ago": "^2.0.1",
    "joi": "^13.0.2",
    "joi-date-extensions": "^1.2.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.5",
    "mailgun-js": "^0.22.0",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "mongoose": "^5.4.5",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "mongoose-cursor-pagination": "^1.0.1",
    "morgan": "^1.9.0",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "multer-s3-transform": "^2.3.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "qs": "^6.9.1",
    "request": "^2.88.0",
    "request-ip": "^2.1.3",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.3",
    "sharp": "^0.21.3",
    "slugify": "^1.3.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "expect": "^22.0.3",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.0",
    "nodemon": "^1.12.5",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0"
  },
  "keywords": [],
  "author": "James Olinya Tobechukwu <olinyajames@gmail.com>",
  "license": "ISC"
}
